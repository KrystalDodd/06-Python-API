   "source": [
    "city_map_clean = city_data_frame.dropna()\n",
    "humidity = city_map_clean[\"Humidity\"].astype(float)\n",
    "locations = city_map_clean[[\"Lat\", \"Lon\"]].astype(float)\n",
    "\n",
    "fig = gmaps.figure()\n",
    "\n",
    "heat_layer = gmaps.heatmap_layer(locations, weights=humidity, \n",
    "                                 dissipating=False, max_intensity=100,\n",
    "                                 point_radius = 1)\n",
    "fig.add_layer(heat_layer)\n",
    "plt.savefig(\"Google Map.png\")\n",
    "fig\n"
   ]
